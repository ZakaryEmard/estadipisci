{
  "name": "estadipisci",
  "version": "1.0.0",
  "description": "A library to help construct a graphql-js server supporting react-relay.",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/ZakaryEmard/estadipisci.git",
  "bugs": {
    "url": "https://github.com/ZakaryEmard/estadipisci.git/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ZakaryEmard/estadipisci.git"
  },
  "main": "lib/index.js",
  "directories": {
    "lib": "./lib"
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE",
    "PATENTS"
  ],
  "options": {
    "mocha": "--require scripts/mocha-bootload src/**/__tests__/**/*.js"
  },
  "babel": {
    "optional": [
      "runtime",
      "es7.asyncFunctions",
      "es7.objectRestSpread"
    ]
  },
  "dependencies": {
    "babel-runtime": "~5.8.3"
  },
  "peerDependencies": {
    "graphql": "~0.4.2"
  },
  "devDependencies": {
    "babel": "5.8.3",
    "babel-core": "5.8.3",
    "babel-eslint": "4.0.5",
    "chai": "3.0.0",
    "chai-as-promised": "5.1.0",
    "coveralls": "2.11.3",
    "eslint": "0.24.0",
    "flow-bin": "0.13.1",
    "graphql": "0.4.2",
    "isparta": "3.0.3",
    "mocha": "2.2.5",
    "sane": "1.1.3"
  },
  "author": "KathryneHaag"
}